{
  "name": "@iziwheel/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"ts-node --transpile-only\" src/index.ts",
    "test": "jest",
    "test:cov": "jest --coverage --passWithNoTests",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "migrate": "prisma migrate dev",
