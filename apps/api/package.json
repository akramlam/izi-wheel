    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/nodemailer": "^6.4.17",
    "@types/qr-image": "^3.2.9",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prisma": "^5.9.1",
    "supertest": "^7.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
