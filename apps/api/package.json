    "stop-backend": "Stop-Process -Id (Get-NetTCPConnection -LocalPort 3001).OwningProcess -Force"
  },
  "dependencies": {
    "@iziwheel/common-types": "workspace:*",
    "@prisma/client": "^5.9.1",
    "@types/axios": "^0.14.4",
