    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.1",
    "pg": "^8.15.6",
    "qr-image": "^3.2.0",
    "qrcode": "^1.5.4",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
